{
    "manifest_version": 3,
    "name": "Screenshot Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "icons": {
      
      "64": "images/icon-64.png"
    
    }
  }
  